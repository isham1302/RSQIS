<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ssaasdasdas"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context=".Upload">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="127dp"
        android:layout_marginTop="50dp"
        app:srcCompat="@drawable/logo"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="40dp"
        android:paddingLeft="2dp"
        android:textColor="#000"
        android:textSize="20dp"
        android:text="Thank you for your time. We appreciate the chance to work with you to resolve the situation.Your complaint has been registered successfully.We will take prompt action"/>
</LinearLayout>